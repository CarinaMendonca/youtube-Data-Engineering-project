{
	"name": "csvcast",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "dataflowcast",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "csv_to_parquet_cast",
						"type": "DatasetReference"
					},
					"name": "castedcsv"
				}
			],
			"transformations": [
				{
					"name": "cast1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          video_id as string,",
				"          trending_date as string,",
				"          title as string,",
				"          channel_title as string,",
				"          category_id as string,",
				"          publish_time as string,",
				"          tags as string,",
				"          views as string,",
				"          likes as string,",
				"          dislikes as string,",
				"          comment_count as string,",
				"          thumbnail_link as string,",
				"          comments_disabled as string,",
				"          ratings_disabled as string,",
				"          video_error_or_removed as string,",
				"          description as string,",
				"          region as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> source1",
				"source1 cast(output(",
				"          video_id as string,",
				"          trending_date as string,",
				"          title as string,",
				"          channel_title as string,",
				"          category_id as integer,",
				"          publish_time as string,",
				"          tags as string,",
				"          views as integer,",
				"          likes as integer,",
				"          dislikes as integer,",
				"          comment_count as integer",
				"     ),",
				"     errors: true) ~> cast1",
				"cast1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'parquet',",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> castedcsv"
			]
		}
	}
}